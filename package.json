{
  "name": "csv-parser-challenge",
  "version": "0.0.1",
  "description": "Simple CSV parser made for coding challenge",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martinek-stepan/CSV-parser.git"
  },
  "keywords": [
    "CSV",
    "Parser"
  ],
  "author": "Stepan Martinek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/martinek-stepan/CSV-parser/issues"
  },
  "homepage": "https://github.com/martinek-stepan/CSV-parser#readme",
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^14.11.2",
    "codecov": "^3.1.0",
    "jest": "^26.5.2",
    "prettier": "^1.13.2",
    "ts-jest": "^26.4.1",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^3.5.3"
  },
  "files": [
    "lib/**/*"
  ]
}
